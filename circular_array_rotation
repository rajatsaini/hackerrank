# Solution to Circular Array Rotation
# Uses the simple Arithmetic to compute these values.
# Once array has been created, it's just manipulation of queried elment from original array.
# one of the neatest program i've written.


import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int k = in.nextInt();
        int q = in.nextInt();
        int[] a = new int[n];
        for(int a_i=0; a_i < n; a_i++){
            a[a_i] = in.nextInt();
        }
        
        // this mod is for test-case #4, its n= 515 & k =100000, so we need to convert it to less than n.
        k = k%n;
        for(int a0 = 0; a0 < q; a0++){
            int m = in.nextInt();
            int query = (n-k+m)%n;
                System.out.println(a[query]);
            }
        
        }
}
